{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Remote Attach to AWS Lambda",
            "type": "debugpy",
            "request": "attach",
            "connect": {
                "port": 5678,
                "host": "localhost",
            },
            "pathMappings": [
                {
                    "localRoot": "${workspaceFolder}/app",
                    "remoteRoot": "/app",
                }
            ]
        }
    ]
}